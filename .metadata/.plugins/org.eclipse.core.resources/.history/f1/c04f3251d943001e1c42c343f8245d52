package com.aurionpro.test;

import com.aurionpro.model.FixedDeposit;
import com.aurionpro.model.HoliInterest;

public class FixedDepositTest {

    public static void main(String[] args) {
        FixedDeposit fd1 = new FixedDeposit(111, "Amisha", 10000, 3, new HoliInterest());
        System.out.println(fd1);
        
        System.out.println("Simple Interest: " + fd1.calculateSimpleInterest());
    }
}
